<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
        自定义配置bean
    -->
    <bean id="consumer01" class="com.example.demo.kafka.PianoConsumer">
        <constructor-arg name="group" value="group-01"/>
        <constructor-arg name="topic" value="Piano"/>
    </bean>

    <bean id="consumer02" class="com.example.demo.kafka.PianoConsumer">
        <constructor-arg name="group" value="group-02"/>
        <constructor-arg name="topic" value="Piano"/>
    </bean>



</beans>
